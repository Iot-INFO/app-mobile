{
  "expo": {
    "name": "Moobi Verde",
    "slug": "moobi-verde",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tomoove.moobiverde",
      "supportsTablet": true
    },
    "android": {
      "package": "com.tomoove.moobiverde",
      "enableDangerousExperimentalLeanBuilds": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBv7PYOI-lImKiDx3D-1eEAUL-B15wS4Bw"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
